####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = byacc
CFLAGS   = -pipe -Wall -W -O2  -DQT_NO_DEBUG
CXXFLAGS = -pipe -W -Wall -g -Wall -W -O2  -DQT_NO_DEBUG
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/share/qt3/mkspecs/default -I. -I. -I/usr/include/qt3
LINK     = g++
LFLAGS   = -Wl,--enable-auto-import
LIBS     = $(SUBLIBS) -L/usr/lib/qt3/lib -lGLU -lqt -lXext -lX11
AR       = ar cqs
RANLIB   = ranlib
MOC      = /usr/lib/qt3/bin/moc
UIC      = /usr/lib/qt3/bin/uic -nounload
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -R
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
