####### Compile

main.o: main.cpp AppWindow.hpp \
		Viewer.hpp

AppWindow.o: AppWindow.cpp AppWindow.hpp \
		Viewer.hpp

Viewer.o: Viewer.cpp Viewer.hpp

moc_AppWindow.o: moc_AppWindow.cpp  AppWindow.hpp Viewer.hpp

moc_Viewer.o: moc_Viewer.cpp  Viewer.hpp 

moc_AppWindow.cpp: $(MOC) AppWindow.hpp
	$(MOC) AppWindow.hpp -o moc_AppWindow.cpp

moc_Viewer.cpp: $(MOC) Viewer.hpp
	$(MOC) Viewer.hpp -o moc_Viewer.cpp

