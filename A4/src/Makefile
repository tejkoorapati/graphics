		/usr/include/lua5.1/luaconf.h \
		/usr/include/lua5.1/lualib.h \
		/usr/include/lua5.1/lauxlib.h \
		light.hpp \
		a4.hpp \
		image.hpp \
		mesh.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o scene_lua.o scene_lua.cpp

mesh.o: mesh.cpp mesh.hpp \
		primitive.hpp \
		algebra.hpp \
		polyroots.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mesh.o mesh.cpp

polyroots.o: polyroots.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o polyroots.o polyroots.cpp

image.o: image.cpp image.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o image.o image.cpp

light.o: light.cpp light.hpp \
		algebra.hpp \
		polyroots.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o light.o light.cpp

a4.o: a4.cpp a4.hpp \
		algebra.hpp \
		polyroots.hpp \
		scene.hpp \
		primitive.hpp \
		material.hpp \
		light.hpp \
		image.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o a4.o a4.cpp

