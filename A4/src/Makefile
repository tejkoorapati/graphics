dist: 
	@test -d .tmp/rt1.0.0 || mkdir -p .tmp/rt1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/rt1.0.0/ && $(COPY_FILE) --parents algebra.hpp lua488.hpp material.hpp primitive.hpp scene.hpp scene_lua.hpp mesh.hpp polyroots.hpp image.hpp light.hpp a4.hpp .tmp/rt1.0.0/ && $(COPY_FILE) --parents algebra.cpp main.cpp material.cpp primitive.cpp scene.cpp scene_lua.cpp mesh.cpp polyroots.cpp image.cpp light.cpp a4.cpp .tmp/rt1.0.0/ && (cd `dirname .tmp/rt1.0.0` && $(TAR) rt1.0.0.tar rt1.0.0 && $(COMPRESS) rt1.0.0.tar) && $(MOVE) `dirname .tmp/rt1.0.0`/rt1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/rt1.0.0


